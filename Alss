function getBypassStatus() local status = "🔍 Status Bypass:\n" status = status .. "• Anti-Login: " .. (bypassState.antiLoginActive and "AKTIF" or "NONAKTIF") .. "\n" status = status .. "• Stabilizer: " .. (bypassState.stabilizerActive and "AKTIF" or "NONAKTIF") .. "\n" status = status .. "• GG Detection: " .. (bypassState.ggDetectionDisabled and "MATI" or "HIDUP")

end

-- ============================================= --MAIN EXECUTION --============================================= local function main() -- Tampilkan welcome screen terlebih dahulu if not showWelcomeScreen() then return -- Keluar jika script expired end

end

-- Jalankan script utama dengan error handling gg.setVisible(false) gg.toast("🎮VELLIX_AO Script Loading...")

local success, err = pcall(main)

if not success then gg.alert("⚠️ CRITICAL ERROR ⚠️\n" .. tostring(err)) else gg.toast("Script selesai dijalankan") end
