-- Fungsi utility untuk modifikasi memory function modifyMemory(pattern,newValue, valueType) local success = pcall(function() gg.searchNumber(pattern, valueType or gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1) local results = gg.getResults(100) if #results > 0 then for _, v in ipairs(results) do v.value = tostring(newValue) end gg.setValues(results) end gg.clearResults() end) return success end

-- Fungsi untuk menampilkan info skill function showSkillInfo(skillId) local skill = MALE_SKILLS[skillId] if skill then local info = string.format([[
